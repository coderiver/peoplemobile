extends include/_layout

block title
	title Voting Closed

block header
	include include/_header-logo-awards

block content
	//begin topnav
	nav.topnav.label-h.label-h_blue
		span.title.title_topnav favorite movie
	//end topnav

	.wrap
		//begin voting closed
		.closed
			.closed__title come back soon!
			p.closed__sub
				strong Voting for [category] has ended.<br>
				| Please come back on [date] when voting opens.
		//end voting closed

		.title.title_news
			span news

		a(href="#").box.box_half-left
			img(src="http://placehold.it/295x295")
			.box__footer.blue
				i.ico.ico_movies
				| Movies
		a(href="#").box.box_half-right
			img(src="http://placehold.it/295x295")
			.box__footer.violet
				i.ico.ico_style
				| Style
		a(href="#").box.box_half-left
			img(src="http://placehold.it/295x295")
			.box__footer.yellow
				i.ico.ico_music
				| Music
		a(href="#").box.box_half-right
			img(src="http://placehold.it/295x295")
			.box__footer.green
				i.ico.ico_tv
				| Tv
